{
  "comments": {
    "lineComment": "#"
  },
  "autoClosingPairs": [
    {
      "open": "'",
      "close": "'",
      "notIn": ["string", "comment"]
    },
    {
      "open": "\"",
      "close": "\"",
      "notIn": ["string"]
    }
  ],
  "autoCloseBefore": ";:.,=}])>` \n\t",
  "surroundingPairs": [
    ["'", "'"],
    ["\"", "\""]
  ],
  "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
  "indentationRules": {
    "increaseIndentPattern": "^\\s*[A-Z][a-zA-Z0-9_]*\\b(?<!End)",
    "decreaseIndentPattern": "^((?!.*?\\/\\*).*\\*/)?\\s*[\\)\\}\\]].*$"
  }
}
